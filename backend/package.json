{
    "name": "dengue-surveillance-backend",
    "version": "1.0.0",
    "description": "Dengue Surveillance & Early Warning System - Backend API",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate deploy",
        "prisma:migrate:dev": "prisma migrate dev",
        "prisma:db:push": "prisma db push --skip-generate",
        "prisma:seed": "tsx src/prisma/seed.ts",
        "prisma:studio": "prisma studio",
        "postinstall": "prisma generate",
        "render-build": "npm install && npm run prisma:generate && npm run build"
    },
    "keywords": [
        "dengue",
        "surveillance",
        "health"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.19.1",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "express": "^4.21.0",
        "express-validator": "^7.2.0",
        "jsonwebtoken": "^9.0.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^22.7.5",
        "dotenv": "^17.2.3",
        "mongodb": "^7.0.0",
        "prisma": "^5.19.1",
        "tsx": "^4.19.1",
        "typescript": "^5.6.3"
    }
}